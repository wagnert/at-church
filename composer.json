{
	"name" : "faett-net/at-church",
	"description" : "The example application for the ApplicationServer",
	"version" : "0.1.0",
	"require" : {
		"components/jquery" : "*",
		"lusitanian/oauth" : "~0.3",
		"monolog/monolog" : "1.12.*",
		"components/font-awesome" : "*",
		"cpliakas/git-wrapper" : "1.4.*",
		"components/bootstrap" : "3.1.1",
		"phpdocumentor/phpdocumentor" : "2.8.1",
		"appserver-io/routlt" : "dev-annotations"
	},
	"require-dev" : {
		"appserver-io/phar" : "0.1.*",
		"appserver-io/build" : "0.2.*"
	},
	"config" : {
		"vendor-dir" : "src/vendor",
		"component-dir" : "src/components"
	},
	"autoload" : {
		"psr-0" : {
			"Net\\Faett\\AtChurch" : [
				"common/classes/",
				"src/WEB-INF/classes/",
				"src/META-INF/classes/",
				"tests/WEB-INF/classes/",
				"tests/META-INF/classes/"
			]
		}
	},
	"authors" : [{
			"name" : "Tim Wagner",
			"email" : "wagner_tim78@hotmail.com"
		}
	],
	"repositories" : [{
			"type" : "git",
			"url" : "git://127.0.0.1/routlt",
			"name" : "appserver-io/routlt"
		}
	]
}